<mxfile><diagram name="Persona 3"><mxGraphModel dx="1200" dy="800"><root><mxCell id="0"/><mxCell id="1" parent="0"/><mxCell id="bg" value="" style="rounded=0;fillColor=#f5f5f5" vertex="1" parent="1"><mxGeometry x="40" y="40" width="1120" height="720"/></mxCell><mxCell id="photo" value="ðŸ§‘" style="ellipse;fillColor=#cd853f;fontSize=80;fontColor=#fff" vertex="1" parent="1"><mxGeometry x="80" y="100" width="200" height="200"/></mxCell><mxCell id="name" value="Emma Chen" style="text;fontSize=32;fontStyle=1" vertex="1" parent="1"><mxGeometry x="320" y="120" width="400" height="40"/></mxCell><mxCell id="demo" value="Age: 28 | Melbourne, VIC\nOccupation: Graphic Designer\nBackground: Art Enthusiast, Chinese-Australian" style="text;fontSize=14;align=left" vertex="1" parent="1"><mxGeometry x="320" y="180" width="500" height="60"/></mxCell><mxCell id="bg-sect" value="Background" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="80" y="340" width="200" height="30"/></mxCell><mxCell id="bg-text" value="Art lover who enjoys traveling within Australia to discover indigenous art. Follows contemporary indigenous artists on social media. Wants to learn about culture respectfully and support indigenous artists." style="rounded=1;fillColor=#fff;align=left;verticalAlign=top" vertex="1" parent="1"><mxGeometry x="80" y="380" width="400" height="100"/></mxCell><mxCell id="goals-sect" value="Goals" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="520" y="340" width="200" height="30"/></mxCell><mxCell id="goals-text" value="âœ“ Discover art locations to visit during travels\nâœ“ Learn cultural significance &amp; stories\nâœ“ Support indigenous artists\nâœ“ Share discoveries on social media (respectfully)\nâœ“ Plan art-focused travel itineraries" style="rounded=1;fillColor=#d5e8d4;align=left;verticalAlign=top;fontSize=13" vertex="1" parent="1"><mxGeometry x="520" y="380" width="360" height="100"/></mxCell><mxCell id="needs-sect" value="Needs" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="80" y="520" width="200" height="30"/></mxCell><mxCell id="needs-text" value="â€¢ Visual, engaging interface with images\nâ€¢ Interactive map showing locations\nâ€¢ Clear directions &amp; access information\nâ€¢ Mobile-friendly for on-the-go use\nâ€¢ Ability to save favorites\nâ€¢ Cultural context for appreciation" style="rounded=1;fillColor=#e3f2fd;align=left;verticalAlign=top;fontSize=13" vertex="1" parent="1"><mxGeometry x="80" y="560" width="400" height="100"/></mxCell><mxCell id="pain-sect" value="Pain Points" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="520" y="520" width="200" height="30"/></mxCell><mxCell id="pain-text" value="â€¢ Uncertainty about appropriate locations\nâ€¢ Worry about being disrespectful\nâ€¢ Difficulty finding comprehensive info\nâ€¢ Unknown if locations are publicly accessible\nâ€¢ Lack of context for full appreciation" style="rounded=1;fillColor=#ffebee;align=left;verticalAlign=top;fontSize=13" vertex="1" parent="1"><mxGeometry x="520" y="560" width="360" height="100"/></mxCell><mxCell id="quote" value='"I want to experience and appreciate indigenous art, but I don\'t want to be an ignorant tourist. I need guidance on how to do this respectfully."' style="rounded=1;fillColor=#fff3e0;fontSize=14;fontStyle=2;align=center" vertex="1" parent="1"><mxGeometry x="920" y="100" width="220" height="180"/></mxCell><mxCell id="tech" value="Tech Level: VERY HIGH\nDigital native, comfortable with apps, maps, social platforms" style="rounded=1;fillColor=#e1d5e7;fontSize=12;align=center" vertex="1" parent="1"><mxGeometry x="920" y="620" width="220" height="60"/></mxCell></root></mxGraphModel></diagram></mxfile>
