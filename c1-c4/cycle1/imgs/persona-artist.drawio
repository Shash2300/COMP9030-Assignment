<mxfile><diagram name="Persona 2"><mxGraphModel dx="1200" dy="800"><root><mxCell id="0"/><mxCell id="1" parent="0"/><mxCell id="bg" value="" style="rounded=0;fillColor=#f5f5f5" vertex="1" parent="1"><mxGeometry x="40" y="40" width="1120" height="720"/></mxCell><mxCell id="photo" value="👨‍🎨" style="ellipse;fillColor=#8b4513;fontSize=80;fontColor=#fff" vertex="1" parent="1"><mxGeometry x="80" y="100" width="200" height="200"/></mxCell><mxCell id="name" value="Uncle Tommy Williams" style="text;fontSize=32;fontStyle=1" vertex="1" parent="1"><mxGeometry x="320" y="120" width="450" height="40"/></mxCell><mxCell id="demo" value="Age: 58 | Arnhem Land, NT\nOccupation: Traditional Artist &amp; Cultural Educator\nBackground: Yolngu Elder" style="text;fontSize=14;align=left" vertex="1" parent="1"><mxGeometry x="320" y="180" width="500" height="60"/></mxCell><mxCell id="bg-sect" value="Background" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="80" y="340" width="200" height="30"/></mxCell><mxCell id="bg-text" value="Respected Yolngu elder who creates traditional bark paintings and teaches younger generations about cultural heritage. Passionate about sharing appropriate cultural knowledge while protecting sacred information." style="rounded=1;fillColor=#fff;align=left;verticalAlign=top" vertex="1" parent="1"><mxGeometry x="80" y="380" width="400" height="100"/></mxCell><mxCell id="goals-sect" value="Goals" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="520" y="340" width="200" height="30"/></mxCell><mxCell id="goals-text" value="✓ Share traditional &amp; contemporary art knowledge\n✓ Document own artwork with cultural stories\n✓ Educate non-indigenous Australians\n✓ Protect sacred sites\n✓ Support other indigenous artists" style="rounded=1;fillColor=#d5e8d4;align=left;verticalAlign=top;fontSize=13" vertex="1" parent="1"><mxGeometry x="520" y="380" width="360" height="100"/></mxCell><mxCell id="needs-sect" value="Needs" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="80" y="520" width="200" height="30"/></mxCell><mxCell id="needs-text" value="• Simple, intuitive interface\n• Clear control over what is shared publicly\n• Ability to mark locations as sensitive\n• Option to provide cultural context\n• Platform that respects protocols" style="rounded=1;fillColor=#e3f2fd;align=left;verticalAlign=top;fontSize=13" vertex="1" parent="1"><mxGeometry x="80" y="560" width="400" height="100"/></mxCell><mxCell id="pain-sect" value="Pain Points" style="text;fontSize=18;fontStyle=1" vertex="1" parent="1"><mxGeometry x="520" y="520" width="200" height="30"/></mxCell><mxCell id="pain-text" value="• Concerns about cultural appropriation\n• Worry about exact locations being shared\n• Complex technology barriers\n• Lack of indigenous voices in platforms\n• Need for proper attribution" style="rounded=1;fillColor=#ffebee;align=left;verticalAlign=top;fontSize=13" vertex="1" parent="1"><mxGeometry x="520" y="560" width="360" height="100"/></mxCell><mxCell id="quote" value='"This is our story, our culture. We want to share it, but it needs to be done right, with respect. Some things are meant for everyone, some things are not."' style="rounded=1;fillColor=#fff3e0;fontSize=14;fontStyle=2;align=center" vertex="1" parent="1"><mxGeometry x="920" y="100" width="220" height="180"/></mxCell><mxCell id="tech" value="Tech Level: MEDIUM\nUses smartphone &amp; social media, prefers simple interfaces" style="rounded=1;fillColor=#e1d5e7;fontSize=12;align=center" vertex="1" parent="1"><mxGeometry x="920" y="620" width="220" height="60"/></mxCell></root></mxGraphModel></diagram></mxfile>
